<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER_LEFT" prefHeight="677.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ruan.caetano.ex1.exController">
   <children>
      <Label text="Razão Social">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="tfRazao" promptText="Informe a razão" />
      <Label layoutX="10.0" layoutY="10.0" text="Nome Fantasia">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="tfNomeFant" layoutX="10.0" layoutY="36.0" promptText="Informe o nome" />
      <HBox spacing="10.0">
         <children>
            <VBox>
               <children>
                  <Label text="CNPJ">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfCNPJ" prefHeight="26.0" prefWidth="209.0" promptText="Informe a razão" />
               </children>
            </VBox>
            <VBox prefHeight="51.0" prefWidth="233.0">
               <children>
                  <Label text="Inscrição estadual">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfIE" prefHeight="26.0" prefWidth="294.0" promptText="Informe a razão" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label layoutX="10.0" layoutY="164.0" text="CEP">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="tfCEP" layoutX="10.0" layoutY="189.0" onKeyReleased="#onCepDigitado" prefHeight="26.0" prefWidth="506.0" promptText="Informe a razão" />
      <HBox>
         <children>
            <VBox>
               <children>
                  <Label text="Endereço">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfEndereco" prefHeight="26.0" prefWidth="434.0" promptText="Informe a razão" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Bairro">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfBairro" prefHeight="26.0" prefWidth="254.0" promptText="Informe a razão" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox>
         <children>
            <VBox>
               <children>
                  <Label text="Cidade">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfCidade" prefHeight="26.0" prefWidth="425.0" promptText="Informe a razão" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Estado">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfEstado" prefHeight="26.0" prefWidth="244.0" promptText="Informe a razão" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label layoutX="10.0" layoutY="420.0" text="Numero">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="tfNumero" layoutX="10.0" layoutY="445.0" promptText="Informe a razão" />
      <Label layoutX="10.0" layoutY="471.0" text="E-mail">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="tfEmail" layoutX="10.0" layoutY="496.0" prefHeight="40.0" prefWidth="600.0" promptText="Informe a razão" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="btnConfirmar" mnemonicParsing="false" onAction="#onConfirmar" prefHeight="26.0" prefWidth="118.0" style="-fx-background-color: #bfddf3;" text="Confirmar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btCancelar" layoutX="231.0" layoutY="20.0" mnemonicParsing="false" onAction="#onCancelar" prefHeight="26.0" prefWidth="118.0" style="-fx-background-color: #ff6961;" text="Cancelar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
